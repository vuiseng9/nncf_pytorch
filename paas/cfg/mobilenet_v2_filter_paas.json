{
    "restful": true,
    "model": "mobilenet_v2",
    "pretrained": true,
    "batch_size" : 512, // eval @ 8.7MB on a single card
    "epochs": 100,
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
   "compression": [
       {
           "algorithm": "filter_pruning",
           "pruning_init": 0.0,
            "params": {
                "weight_importance": "geometric_median",
                "prune_downsample_convs" : true,
                "prune_first_conv": true,
                "prune_last_conv": true,
                "prune_batch_norms": true,
            }
        }
   ]
}
